(section
  (ref "jack_port_name")
  (title "Jack.Port.Name")
  (subsection
    (title "Synopsis")
    (para-verbatim function_synopsis
"with Jack.Port;

function Name (Port : in Jack.Client.Port_t) return Jack.Client.Port_Name_t;
"))
  (subsection
    (title "Description")
    (para
      "The " (item ada_function "Name") " function retrieves the
      name of " (item variables "Port") ".")
  (subsection
    (title "Mapping")
    (para
      "The " (item ada_function "Name") " function is equivalent to "
      (item c_function "jack_port_name") "."))))
